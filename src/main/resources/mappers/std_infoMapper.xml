<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.akisan.uploadProjectData.mapper.std_infoMapper">
    <resultMap id="BaseResultMap" type="com.akisan.uploadProjectData.entity.std_info">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="classname" property="classname"/>
        <result column="inschool" property="inschool"/>
        <result column="passrate" property="passrate"/>
        <result column="actvrate" property="actvrate"/>
    </resultMap>

    <insert id="insertFakeData">
        INSERT INTO std_info(name, classname, inschool)
        VALUES
        <foreach collection="std_info" item="std_info" separator=",">
            (#{std_info.name,jdbcType=VARCHAR}, #{std_info.classname, jdbcType=VARCHAR}, #{std_info.inschool, jdbcType=VARCHAR})
        </foreach>
    </insert>
</mapper>